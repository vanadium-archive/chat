{
  "camelcase": true,
  "eqeqeq": true,
  "expr": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxlen": 80,
  "newcap": true,
  "noarg": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "sub": true,
  "trailing": true,
  "undef": true,
  "unused": "vars",
  "esnext": true,
  "browser": true,
  "devel": true,
  "node": true,

  "globals": {
    "Promise": true
  }
}
